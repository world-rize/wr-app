rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      allow read, write: if request.auth.token.admin
    }

    match /users/{user} {
      allow read, write: if request.auth != null && request.auth.uid == user
    }

    // // masterdata
    // match /etc/{etc} {
    //   allow read: if true
    // }
    // match /newcoming/{newcoming} {
    //   allow read: if true
    // }
    // match /shop/{shop} {
    //   allow read: if true
    // }
  }
}