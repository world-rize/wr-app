                }
              ],
              "data": {}
            },
            {
              "nodeType": "embedded-asset-block",
              "content": [],
              "data": {
                "target": {
                  "sys": {
                    "id": "3KTGWQyzbxLk8SravXaiqf",
                    "type": "Link",
                    "linkType": "Asset"
                  }
